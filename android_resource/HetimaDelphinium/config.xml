<?xml version='1.0' encoding='utf-8'?><widget id="net.hetimatan.dart.delphinium" version="0.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HetimaDelphinium</name>
    <description>Plain text description of this app</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Author Name &lt;a@b.com></author>
    <content src="plugins/cordova-plugin-chrome-apps-bootstrap/chromeapp.html"/>
    <plugin name="cordova-plugin-whitelist" version="1"/>
    
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    <icon src="www/dart_icon.png"/><icon src="www/dart_icon.png" width="128"/></platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    <icon src="www/dart_icon.png"/><icon src="www/dart_icon.png" width="128"/></platform>
<preference name="android-minSdkVersion" value="14"/></widget>